{
  "name": "rimraf-glob-bug-showcase",
  "version": "1.0.0",
  "description": "This is a pure inconsistency showcasing repository for the all famous \"rimraf\" npm package",
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm run remove-all && node src/index.js",
    "test": "rimraf generated/*.js",
    "test:fixed": "rimraf generated/*.js --glob",
    "remove-all": "rimraf generated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malinovic/rimraf-glob-bug-showcase.git"
  },
  "keywords": [
    "rimraf",
    "bug",
    "glob"
  ],
  "author": "Danijel Malinovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malinovic/rimraf-glob-bug-showcase/issues"
  },
  "homepage": "https://github.com/malinovic/rimraf-glob-bug-showcase#readme",
  "dependencies": {
    "rimraf": "5.0.5"
  }
}
